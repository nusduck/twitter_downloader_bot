# Telegram Bot Token from @BotFather
BOT_TOKEN=your_bot_token_here

# Local Bot API Server URL (Required for >50MB uploads, up to 2GB)
# Format: http://host:port/bot
# If using standard API, leave as is or empty
BOT_API_URL=http://localhost:8081/bot

# Allowed user IDs (optional, comma separated)
ALLOWED_USERS=12345678,98765432

# Twitter/X cookies (if needed for some scrapers)
TWITTER_COOKIES=

# Path to store temporary downloads
DOWNLOAD_PATH=./downloads
